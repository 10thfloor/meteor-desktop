{
    "name": "meteor-electron-desktop-client",
    "version": "0.0.25",
    "bin": {
        "meteor-desktop": "dist/bin/cli.js"
    },
    "engines": {
        "node": ">=4.0.0"
    },
    "description": "Build a Meteor's desktop client with hot code push.",
    "main": "dist/index.js",
    "scripts": {
        "test": "npm run build && mocha --compilers js:babel-core/register \"tests/+(integration|unit)/**/*.test.js\"",
        "test-watch": "watch \"npm run test\" lib tests/integration tests/unit",
        "prepublish": "npm run build",
        "build": "babel lib --out-dir dist --source-maps inline --copy-files",
        "build-watch": "babel lib --watch --out-dir dist --source-maps inline --copy-files"
    },
    "author": "Bartosz Wojtkowiak <bartosz@wojtkowiak.it>",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.9.0",
        "babel-core": "^6.9.0",
        "babel-eslint": "^6.0.4",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015-without-strict": "0.0.2",
        "babel-preset-stage-1": "^6.5.0",
        "babel-plugin-transform-runtime": "^6.9.0",
        "chai": "^3.5.0",
        "del": "^2.2.0",
        "dirty-chai": "^1.2.2",
        "eslint": "^2.10.2",
        "eslint-config-airbnb-base": "^3.0.1",
        "eslint-plugin-import": "^1.8.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-load-plugins": "^1.2.4",
        "gulp-mocha": "^2.2.0",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-util": "^3.0.7",
        "merge-stream": "^1.0.0",
        "private-decorator": "^0.1.5",
        "run-sequence": "^1.2.1",
        "sinon": "^1.17.4",
        "sinon-chai": "^2.8.0",
        "watch": "^0.18.0"
    },
    "dependencies": {
        "commander": "2.9.0",
        "cross-spawn": "3.0.1",
        "electron-packager": "7.0.3",
        "electron-prebuilt": "1.2.2",
        "lodash": "4.13.1",
        "private-decorator": "0.1.5",
        "semver": "5.1.0",
        "semver-regex": "1.0.0",
        "shelljs": "0.7.0",
        "extfs": "0.0.7",
        "single-line-log": "1.1.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wojtkowiak/meteor-electron-desktop-client"
    },
    "keywords": [
        "electron",
        "meteor",
        "packager",
        "desktop"
    ],
    "bugs": {
        "url": "https://github.com/wojtkowiak/meteor-electron-desktop-client/issues"
    },
    "homepage": "https://github.com/wojtkowiak/meteor-electron-desktop-client"
}
