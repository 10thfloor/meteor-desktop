{
  "name": "meteor-desktop",
  "version": "0.0.41",
  "bin": {
    "meteor-desktop": "dist/bin/cli.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "description": "Build a Meteor's desktop client with hot code push.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register \"tests/+(functional|unit)/**/*.test.js\"",
    "test-watch": "watch \"npm run test\" --wait 10 dist tests/functional tests/unit scaffold skeleton",
    "prepublish": "npm run build",
    "build": "babel lib --out-dir dist --source-maps inline --copy-files",
    "build-watch": "babel lib --watch --out-dir dist --source-maps inline --copy-files",
    "develop": "npm-run-all --parallel build-watch test-watch"
  },
  "author": "Bartosz Wojtkowiak <bartosz@wojtkowiak.it>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "chai": "^3.5.0",
    "connect": "3.4.1",
    "connect-modrewrite": "0.9.0",
    "del": "^2.2.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "find-port": "2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-load-plugins": "^1.2.4",
    "gulp-mocha": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "merge-stream": "^1.0.0",
    "mockery": "^1.7.0",
    "node-fetch": "1.5.3",
    "npm-run-all": "^2.1.1",
    "private-decorator": "^0.1.5",
    "proxyquire": "^1.7.9",
    "queue": "4.0.0",
    "request": "2.72.0",
    "rewire": "^2.5.1",
    "run-sequence": "^1.2.1",
    "serve-static": "1.10.2",
    "server-destroy": "1.0.1",
    "sha1-file": "1.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "asar": "^0.11.0",
    "babel-core": "6.9.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-node6": "11.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "6.9.2",
    "commander": "2.9.0",
    "cross-spawn": "3.0.1",
    "del": "2.2.1",
    "electron-packager": "7.2.0",
    "electron-prebuilt": "1.2.5",
    "extfs": "0.0.7",
    "glob": "7.0.5",
    "hash-files": "1.1.1",
    "lodash": "4.13.1",
    "npm3": "1.1.0",
    "private-decorator": "0.1.5",
    "semver": "5.1.0",
    "semver-regex": "1.0.0",
    "shelljs": "0.7.0",
    "single-line-log": "1.1.1",
    "uglify-js": "^2.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wojtkowiak/meteor-desktop"
  },
  "keywords": [
    "electron",
    "meteor",
    "packager",
    "desktop"
  ],
  "bugs": {
    "url": "https://github.com/wojtkowiak/meteor-desktop/issues"
  },
  "homepage": "https://github.com/wojtkowiak/meteor-desktop"
}
