{
    "name": "My Meteor App",
    "projectName": "MyMeteorApp",
    "devTools": true,
    "devtron": true,
    "desktopHCP": true,
    "desktopHCPSettings": {
        "ignoreCompatibilityVersion": false,
        "blockAppUpdateOnDesktopIncompatibility": true
    },
    "webAppStartupTimeout": 60000,
    "window": {
        "fullscreen": false,
        "kiosk": false,
        "alwaysOnTop": false,
        "icon": "meteor.png",
        "_windows": {
            "icon": "meteor.ico"
        }
    },
    "packageJsonFields": {
        "description": "My Meteor App",
        "author": "Me, Myself And I",
        "private": true
    },
    "builderOptions": {
        "win": {
            "iconUrl": "https://github.com/wojtkowiak/meteor-desktop/blob/beta/scaffold/assets/meteor.ico?raw=true",
            "title": "My Meteor App",
            "authors": "Me, Myself And I"
        },
        "icon": "../.desktop/assets/meteor.ico",
        "appId": "meteor.app",
        "category": "meteor.apps",
        "mac": {
            "icon": ".desktop/assets/meteor.icns"
        },
        "dmg": {
            "icon": ".desktop/assets/meteor.icns",
            "background": ".desktop/assets/splashScreen.png"
        }
    },
    "packagerOptions": {
        "icon": "meteor.ico",
        "_osx": {
            "icon": "meteor.icns"
        },
        "version-string": {
            "CompanyName": "My Company",
            "LegalCopyright": "Copyright 2016 My Company - All rights reserved.",
            "FileDescription": "MyMeteorApp",
            "OriginalFilename": "MyMeteorApp.exe",
            "FileVersion": "@version",
            "ProductVersion": "@version",
            "ProductName": "MyMeteorApp",
            "InternalName": "MyMeteorApp.exe"
        }
    },
    "plugins": {
        "meteor-desktop-splash-screen": {
            "version": "0.0.20"
        }
    },
    "uglify": true,
    "version": "0.0.1",
    "dependencies": {}
}
